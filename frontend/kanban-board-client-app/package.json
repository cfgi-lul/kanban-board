{
  "name": "kanban-board-client-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:watch": "ng test --watch",
    "test:coverage": "ng test --code-coverage",
    "lint": "eslint . --ext .ts,.html --config config/eslint/eslint.config.js",
    "lint:fix": "eslint . --ext .ts,.html --fix --config config/eslint/eslint.config.js",
    "format": "prettier --write \"src/**/*.{ts,html,scss,json}\" --config config/prettier/.prettierrc",
    "format:check": "prettier --check \"src/**/*.{ts,html,scss,json}\" --config config/prettier/.prettierrc",
    "stylelint": "stylelint \"src/**/*.{css,scss}\" --config config/stylelint/.stylelintrc.json",
    "stylelint:fix": "stylelint \"src/**/*.{css,scss}\" --fix --config config/stylelint/.stylelintrc.json",
    "generate-api": "openapi-generator-cli version && openapi-generator-cli generate -c ./swagger/openapi-gen-config.json",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/kanban-board-client-app/stats.json",
    "analyze:build": "ng build --stats-json",
    "analyze:start": "webpack-bundle-analyzer dist/kanban-board-client-app/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.4",
    "@angular/cdk": "^20.1.4",
    "@angular/common": "^20.1.4",
    "@angular/compiler": "^20.1.4",
    "@angular/core": "^20.1.4",
    "@angular/forms": "^20.1.4",
    "@angular/material": "^20.1.4",
    "@angular/platform-browser": "^20.1.4",
    "@angular/platform-browser-dynamic": "^20.1.4",
    "@angular/router": "^20.1.4",
    "@auth0/angular-jwt": "^5.2.0",
    "@ngx-translate/core": "^16.0.0",
    "@ngx-translate/http-loader": "^16.0.0",
    "@stomp/stompjs": "^7.1.1",
    "ngx-markdown": "^20.0.0",
    "ngx-permissions": "^19.0.0",
    "rxjs": "~7.8.2",
    "sockjs-client": "^1.6.1",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-builders/jest": "^20.0.0",
    "@angular-devkit/build-angular": "^20.1.4",
    "@angular/cli": "^20.1.4",
    "@angular/compiler-cli": "^20.1.4",
    "@compodoc/compodoc": "^1.1.26",
    "@eslint/js": "^9.32.0",
    "@openapitools/openapi-generator-cli": "^2.21.4",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preset-angular": "^14.0.0",
    "ngx-spec": "^2.1.6",
    "prettier": "^3.6.2",
    "stylelint": "^14.16.1",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0",
    "stylelint-config-prettier": "^9.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "~5.8.3",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
